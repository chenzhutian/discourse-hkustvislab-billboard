{{#if showBillBoard }}
<div id="whos-online">
	<table>
		<tr>
			<td></td>
			<td>{{whos-online-avatar user=targetUser}}</td>
			<td>{{whos-online-avatar user=user}}</td>
		</tr>
		{{#each summaryTableRows as |row|}}
		<tr>
			<td>{{row.key}}</td>
			<td>{{row.targetValue}}</td>
			<td>{{row.value}}</td>
		</tr>
		{{/each}}
	</table>
</div>
{{/if}}